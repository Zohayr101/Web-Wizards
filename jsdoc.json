{
    "tags": {
      "allowUnknownTags": true
    },
    "source": {
      "include": [
        "public/js",
        "src"
      ],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)node_modules($|\\/|\\\\)"
    },
    "opts": {
      "destination": "./docs",
      "encoding": "utf8",
      "recurse": true
    }
  }
  